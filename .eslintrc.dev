{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-unused-prop-types" : 0,
    "react/forbid-prop-types": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "func-names": 0,
    "no-console": 0,
    "no-use-before-define": 0,
  },
  "env": {
    "browser": true,
    "node": true
  }
}